{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "./logs/log-{Hour}.txt" }
      }
    ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "CosmosDb": {
	"DatabaseName": "ToDoList",
	"ContainerName": "Items",
	"Account": "",
	"Key": ""
  },
  "AllowedHosts": "*"
}
